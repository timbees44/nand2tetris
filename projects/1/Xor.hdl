/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=out1);
    Not(in=b, out=out2);
    And(a=b, b=out1, out=out3);
    And(a=out2, b=a, out=out4);
    Or(a=out3, b=out4, out=out);
}
